# Path to the pending transactions queue JSON file (must be writable on this server)
QUEUE_PATH=./data/pending-queue.json

# Path to the invoice queue JSON file (must be writable on this server)
INVOICE_QUEUE_PATH=./data/invoice-queue.json

# Agent signer private key (for POST /execute co-signing)
AGENT_PRIVATE_KEY=

# Pimlico API key (bundler / paymaster for BSC)
PIMLICO_API_KEY=


# Server
PORT=3001

# Path to patterns.json for risk analysis (also determines skills directory for record-pattern.js)
PATTERNS_PATH=

# Allowed origins for CORS (comma-separated). Default: http://localhost:3000, http://127.0.0.1:3000
# CORS_ORIGIN=https://your-app.vercel.app,https://www.example.com
